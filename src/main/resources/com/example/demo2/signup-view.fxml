                        <Font name="Bookman Old Style" size="40.0" />
                    </font>
                </Text>
                <TextField fx:id="name" layoutX="51.0" layoutY="93.0" prefHeight="25.0" prefWidth="273.0" promptText="Name" style="-fx-background-color: transparent; -fx-border-color: #1B4242; -fx-border-width: 0 0 1 0;" />
